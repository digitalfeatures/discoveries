{
  "manifest_version": 3,
  "name": "Discovery the world",
  "short_name": "Discoveries",
  "description": "discover another side of the world!",
  "homepage_url": "https://www.producthunt.com/products/365-design-tips",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "version": "0.0.1",
  "permissions": ["storage"],
  "host_permissions": [
    "https://xyzrank.com/*",
    "https://www.xiaoyuzhoufm.com/*",
    "https://www.producthunt.com/*"
  ],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "Discovery the world"
  },
  "chrome_url_overrides": {
    "newtab": "dist/pages/discovery/index.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
